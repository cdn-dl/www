{
   "name": "ad-center",
   "version": "0.1.1",
   "main": "",
   "license": "MIT",
   "bin": {
      "ad-center": "dist/server.js"
   },
   "scripts": {
      "test": "mocha 'tests/**/test.*.ts' ",
      "build": "run-s build:api build:admin build:app",
      "build:api": "webpack --mode=production --node-env=production",
      "build:admin": "cd plugins/admin-view && yarn build",
      "build:app": "cd plugins/app-view && yarn build",
      "build:dev": "webpack --mode=development",
      "build:prod": "webpack --mode=production --node-env=production",
      "zip": "npx node-zip ./dist iproxy-server.zip",
      "watch": "webpack --watch",
      "serve": "webpack serve",
      "start": "run-p start:api start:admin start:app  ",
      "start:view": "run-p start:admin start:app  ",
      "start:api": "cross-env NODE_ENV=development npx ts-node-dev bin/start.ts",
      "start:admin": "cd plugins/admin-view && yarn start",
      "start:app": "cd plugins/app-view && yarn start",
      "format": "run-p format:*",
      "format:js": " prettier --write   \"src/**/*.ts\"  ",
      "format:admin": "cd plugins/admin-view && yarn format",
      "format:app": "cd plugins/app-view && yarn format",
      "deploy": "run-s deploy:api deploy:admin deploy:app",
      "deploy:api": "yarn",
      "deploy:admin": "cd plugins/admin-view && yarn",
      "deploy:app": "cd plugins/app-view && yarn"
   },
   "dependencies": {
     
   },
   "devDependencies": {
      "@koa/cors": "^3.3.0",
      "@types/koa": "^2.13.5",
      "@types/mocha": "^9.0.0",
      "@types/node": "^17.0.32",
      "@webpack-cli/generators": "^2.2.0",
      "commander": "^8.1.0",
      "copy-webpack-plugin": "9.0.1",
      "cross-env": "^7.0.3",
      "dayjs": "^1.11.2",
      "filemanager-webpack-plugin": "^8.0.0",
      "https-proxy-agent": "^5.0.1",
      "jsonwebtoken": "^8.5.1",
      "koa": "^2.13.4",
      "koa-body": "4.2.0",
      "koa-bodyparser": "^4.3.0",
      "koa-compress": "^5.1.0",
      "koa-router": "^10.1.1",
      "koa-static": "^5.0.0",
      "koa-swagger-decorator": "^1.8.6",
      "mkdirs": "^0.0.3",
      "mocha": "^9.0.3",
      "nanoid": "^3.2.0",
      "npm-run-all": "^4.1.5",
      "prettier": "^2.3.2",
      "ts-loader": "^9.2.4",
      "ts-node": "^10.1.0",
      "ts-node-dev": "^2.0.0",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "^4.3.5",
      "webpack": "^5.52.0",
      "webpack-cli": "^4.8.0",
      "webpack-dev-server": "^4.1.0"
   },
   "description": "demo",
   "mocha": {
      "require": [
         "ts-node/register"
      ],
      "ui": "bdd",
      "reporter": "spec"
   },
   "files": [
      "dist",
      "package.json",
      "README.md"
   ]
}
